{
  "run": [{
      "method": "input",
      "params": {
        "title": "Starting...",
        "description": "The installation of the dependencies will start now..."
      }
    }, {
      "method": "shell.start",
      "params": {
        "path": "",
        "env": {
          "HF_HOME": "../"
        }
      }
    }, {
    "method": "shell.enter",
    "params": {
      "message": "dir",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "conda create -y -n gaussian-splatting python=3.10",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "call activate gaussian-splatting",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "conda install -y pytorch=1.12.1 torchvision=0.13.1 torchaudio=0.12.1 pytorch-cuda=11.8 plyfile libffi tqdm -c pytorch -c conda-forge -c nvidia -c defaults",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "conda install -y cudnn cudatoolkit -c nvidia",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "conda install -y colmap -c conda-forge",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "pip install simple_knn-0.0.0-cp310-cp310-win_amd64.whl",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "pip install diff_gaussian_rasterization-0.0.0-cp310-cp310-win_amd64.whl",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "input",
    "params": {
      "title": "Install Success",
      "description": "Go back to the dashboard and launch the app!"
    }
  }]
}
