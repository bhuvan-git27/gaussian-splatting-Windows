{
  "run": [{
    "method": "shell.run",
    "params": {
      "message": "git clone --recurse-submodules https://github.com/graphdeco-inria/gaussian-splatting"
    }
  }, {
    "method": "shell.start",
    "params": {
      "path": "gaussian-splatting"
    }
  }, {
      "method": "shell.enter",
      "params": {
        "message": "{{os.platform() === 'win32' ? 'conda_hook' : 'eval \"$(conda shell.bash hook)\"'}}",
        "on": [{
          "event": null,
          "return": true
        }]
      }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "conda create -y -f {{path.resolve(cwd, 'gaussian-splatting', 'environment.yml')}} -p {{path.resolve(cwd, 'gaussian-splatting', 'conda_env')}}",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "conda activate {{path.resolve(cwd, 'gaussian-splatting', 'conda_env')}}",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "conda install -y cudnn cudatoolkit -c nvidia",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "conda install -y colmap imagemagick -c conda-forge",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "input",
    "params": {
      "title": "Install Success",
      "description": "Go back to the dashboard and launch the app!"
    }
  }, {
    "method": "browser.open",
    "params": {
      "uri": "/?selected=facefusion"
    }
  }]
}